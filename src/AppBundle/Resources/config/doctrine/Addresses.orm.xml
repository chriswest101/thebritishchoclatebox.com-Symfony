<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Addresses" table="addresses">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="emailAddress" type="string" column="email_address" length="80" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="80" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address1" type="string" column="address_1" length="190" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address2" type="string" column="address_2" length="190" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="town" type="string" column="town" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="county" type="string" column="county" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="postcode" type="string" column="postcode" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
